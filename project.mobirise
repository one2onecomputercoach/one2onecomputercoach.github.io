      "settings": {
        "title": "Ananda by Design | Primordial Sound Meditation",
        "order": 3,
        "meta_descr": "Ananda by Design | What is Primordial Sound Meditation | Chopra Certified Meditation Instructor | Meditation Teacher in Palm Springs"
      },
      "components": [
        {
          "_inherit": "header1",
          "_params": {
            "showTitle": true,
            "showText": true,

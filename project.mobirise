        "meta_descr": "Ananda by Design | Meditation with Joanna | Chopra Certified Meditation Instructor | What is Primordial Sound Meditation"
      },
      "components": [
        {
          "_inherit": "header1",
          "_params": {
            "showTitle": true,
            "showText": true,
            "showButtons": true,
            "showArrow": false,
            "contentAlign": "center",
            "bgImageRadio": true,
